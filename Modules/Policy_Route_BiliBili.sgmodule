[Rule]

####################################
# 拦截规则
####################################

#⚠️ Beacon
# > https://data.bilibili.com/log/web?
# > https://data.bilibili.com/v2/log/web?
# > https://data.bilibili.com/log/pbweb/realtime?web
DOMAIN, data.bilibili.com, REJECT

#⚠️ 未知
# > wss://hw-v2-web-player-tracker.biliapi.net/socket.io/?
DOMAIN-SUFFIX, hw-v2-web-player-tracker.biliapi.net, REJECT

#⚠️ 未知
# > wss://ali-web-player-tracker.biliapi.net/socket.io/?
DOMAIN-SUFFIX, ali-web-player-tracker.biliapi.net, REJECT


DOMAIN-SUFFIX, dataflow.biliapi.com, REJECT

#⚠️ 广告图片存放位置
# > https://i0.hdslb.com/bfs/sycp/creative_img/
URL-REGEX, ^https?:\/\/i0\.hdslb\.com\/bfs\/sycp\/creative_img\/, REJECT

#⚠️ 移动端 HTML5 页面下方的打开BiliBili状态栏
# > https://s1.hdslb.com/bfs/static/jinkela/mstation-h5/css/mstation
# URL-REGEX, ^https:\/\/s1\.hdslb\.com\/bfs\/static\/jinkela\/mstation-h5\/css\/mstation, REJECT-DICT

# 字幕，弹幕
# wss://broadcast.chat.bilibili.com:7826/sub

DOMAIN-SUFFIX, cm.bilibili.com, REJECT

URL-REGEX, ^https?:\/\/api\.bilibili\.com\/x\/click-interface\/click/h5, REJECT

URL-REGEX, ^https?:\/\/api\.bilibili\.com\/x\/report\/web\/heartbeat, REJECT

URL-REGEX, ^https?:\/\/api\.bilibili\.com\/x\/click\-interface\/web\/heartbeat, REJECT

URL-REGEX, ^https?:\/\/api\.bilibili\.com\/x\/v2\/dm\/ad, REJECT

# 可能的一些推广(beta)
URL-REGEX, ^https?:\/\/api\.bilibili\.com\/pgc\/season\/app\/related\/recommend\?, REJECT

# URL Rewrite
# 去除搜索中的大家都在搜
URL-REGEX, ^https?:\/\/api\.vc\.bilibili\.com\/search_svr\/v\d\/Search\/recommend_words, REJECT

# 去除动态中的话题
URL-REGEX, ^https?:\/\/api\.vc\.bilibili\.com\/topic_svr\/v1\/topic_svr, REJECT

# 去除动态中的最常访问
URL-REGEX, ^https?:\/\/api\.vc\.bilibili\.com\/dynamic_svr\/v1\/dynamic_svr\/mix_uplist, REJECT

# 可能的一些推广(beta)
URL-REGEX, ^https?:\/\/api\.bilibili\.com\/pgc\/season\/app\/related\/recommend\?, REJECT

# BiliBili漫画去广告
URL-REGEX, ^https?:\/\/manga\.bilibili\.com\/twirp\/comic\.v\d\.Comic\/Flash, REJECT
URL-REGEX, ^https?:\/\/manga\.bilibili\.com\/twirp\/comic\.v\d\.Comic\/ListFlash, REJECT


